	</servlet-mapping>

	<servlet>
		<servlet-name>ControllerTypeServlet</servlet-name>
		<servlet-class>ctrl.ControllerType</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControllerTypeServlet</servlet-name>
		<url-pattern>/ControllerType</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ControllerMediumServlet</servlet-name>
		<servlet-class>ctrl.ControllerMedium</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControllerMediumServlet</servlet-name>
		<url-pattern>/ControllerMedium</url-pattern>
	</servlet-mapping>
</web-app>